{
	"name": "delta-language",
	"displayName": "Delta Language",
	"description": "Support for the Delta language",
	"version": "0.1.0",
	"publisher": "delta-lang",
	"engines": {
		"vscode": "^1.17.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"url": "https://github.com/delta-lang/delta-vscode"
	},
	"contributes": {
		"languages": [
			{
				"id": "delta",
				"aliases": [
					"Delta",
					"delta"
				],
				"extensions": [
					".delta"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "delta",
				"scopeName": "source.delta",
				"path": "./syntaxes/delta.json"
			}
		],
		"snippets": [
			{
				"language": "delta",
				"path": "./snippets/snippets.json"
			}
		]
	}
}
