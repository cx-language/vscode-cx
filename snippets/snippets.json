{
  "Interface declaration": {
    "prefix": "interface",
    "body": ["interface $1 {", "\t$0", "}"],
    "description": "Interface declaration"
  },
  "Enum declaration": {
    "prefix": "enum",
    "body": ["enum $1 {", "\t$0", "}"],
    "description": "Enum declaration"
  },
  "Struct declaration": {
    "prefix": "struct",
    "body": ["struct $1 {", "\t$0", "}"],
    "description": "Struct declaration"
  },
  "Variable declaration": {
    "prefix": "var",
    "body": ["var $1 = $0;"],
    "description": "Variable declaration"
  },
  "Constant declaration": {
    "prefix": "const",
    "body": ["const $1 $2 = $0;"],
    "description": "Constant declaration"
  },
  "Function declaration": {
    "prefix": "fun",
    "body": ["$1 $2($3) {", "\t$0", "}"],
    "description": "Function declaration"
  },
  "while": {
    "prefix": "while",
    "body": ["while ($1) {", "\t$0", "}"],
    "description": ""
  },
  "For loop": {
    "prefix": "for",
    "body": ["for (var $1 in $2) {", "\t$0;", "}"],
    "description": "For loop"
  },
  "Defers execution of a method": {
    "prefix": "defer",
    "body": ["defer $1($0);"],
    "description": "Defers execution of a method"
  },
  "switch": {
    "prefix": "switch",
    "body": ["switch ($1) {", "default:", "\t$0", "}"],
    "description": "Code snippet for switch statement"
  },
  "if": {
    "prefix": "if",
    "body": ["if ($1) {", "\t$0", "}"],
    "description": "Code snippet for if statement"
  },
  "else": {
    "prefix": "else",
    "body": ["else {", "\t$0", "}"],
    "description": "Code snippet for else statement"
  },
  "else if": {
    "prefix": "else if",
    "body": ["else if ($1) {", "\t$0", "}"],
    "description": "Code snippet for else-if statement"
  },
  "ifelse": {
    "prefix": "ifelse",
    "body": "\nif ($1) {\n\t$2\n} else {\n\t$3\n}\n",
    "description": "if ... else ...",
    "scope": "source.delta"
  },
  "#if": {
    "prefix": "#if",
    "body": ["#if $1", "$2", "#endif"],
    "description": "Code snippet for #if"
  },
  "#ifelse": {
    "prefix": "#ifelse",
    "body": ["#if $1", "$2", "#else", "$3", "#endif"],
    "description": "Code snippet for #if"
  }
}
